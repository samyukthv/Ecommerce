                                      <a href="/addToCart/{{this.productdt._id}}">  <button value="" class="btn_3" style=" padding: 5px 10px; font-size: 14px; width: 120px;">
                                            add to cart
                                        </button></a>
                                    </div>
                                </td>
                                 <td>
                                    <a style="font-size: 30px;" href="/deletewish/{{this.productdt._id}}"><i  class="fa-regular fa-circle-xmark"></i></a>
                                 </td>
                                 
                                
